<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.psi.quetzalkitchen.LoginController">
   <children>
      <Text layoutX="197.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Usuario:">
         <font>
            <Font size="14.0" />
         </font>
      </Text>
      <TextField fx:id="userMail" layoutX="300.0" layoutY="102.0" promptText="Correo electrónico" />
      <Text layoutX="199.0" layoutY="185.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Contraseña" />
      <TextField fx:id="userPass" layoutX="301.0" layoutY="168.0" promptText="Contraseña" />
      <Button fx:id="confirm" layoutX="259.0" layoutY="245.0" mnemonicParsing="false" onAction="#confirm" text="Confirmar" />
      <Button fx:id="registry" layoutX="158.0" layoutY="300.0" mnemonicParsing="false" onAction="#switchToRegistry" text="No tengo cuenta" />
      <Button fx:id="delete" layoutX="349.0" layoutY="300.0" mnemonicParsing="false" onAction="#deleteAll" text="Borrar" />
   </children>
</AnchorPane>
